mixin videos()
	#videos.row.valign
		.offset-lg-4
			video(id="my-player", class="video-js", controls, preload="auto", poster="//vjs.zencdn.net/v/oceans.png", data-setup='{}')
				source(src="//vjs.zencdn.net/v/oceans.mp4" type="video/mp4")
				source(src="//vjs.zencdn.net/v/oceans.mp4" type="video/ogv")
				source(src="//vjs.zencdn.net/v/oceans.mp4" type="video/webm")
				track(label="English" kind="subtitles" srclang="en" src="en.vtt" default)
